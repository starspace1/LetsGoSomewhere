<% if @busy_interval %>
console.log("Created event " + "<%= @busy_interval.id %>")
  $('#calendar').fullCalendar('renderEvent', {
                          id: <%= @busy_interval.id %>,
                          title: 'Busy',
                          start: "<%= @busy_interval.start_time %>",
                          end: "<%= @busy_interval.end_time %>",
                          allDay: true,
                          rendering: "background",
                          color: "red",
                          backgroundColor: "red"
                      }, false );
<% end %>
